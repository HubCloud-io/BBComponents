@typeparam TValue

<input @ref="_inputReference"
       id="@Id"
       class="bb-number-input @HtmlClass"
       style="@HtmlStyle"
       readonly=@IsReadOnly
       disabled=@IsDisabled
       data-toggle="tooltip"
       title="@Tooltip"
       placeholder="@Placeholder"
       value="@_stringValue"
       @onkeydown="(e=> OnKeyDown(e))"
       @onchange="(e => OnValueChange(e))" 
       @onfocus="OnInputFocus"/>


