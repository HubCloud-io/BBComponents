@typeparam TValue

<input id="@Id"
       class="bb-number-input @HtmlClass"
       style="@HtmlStyle"
       readonly=@IsReadOnly
       disabled=@IsDisabled
       data-toggle="tooltip"
       title="@Tooltip"
       placeholder="@Placeholder"
       value="@_stringValue"
       @onchange="(e => OnValueChange(e))" />


